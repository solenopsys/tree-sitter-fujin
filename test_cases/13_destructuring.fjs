// Test 13: Destructuring inside actor
type Point = { x: i32, y: i32 };
type DistanceMsg = { point: Point };
type DistanceOut = { type: "distance", value: i32 };

actor distance(msg: DistanceMsg) {
  const {x, y} = msg.point;
  const [a, b] = [1, 2];
  const out: DistanceOut = { type: "distance", value: x + y + a + b };
  emit out;
}
